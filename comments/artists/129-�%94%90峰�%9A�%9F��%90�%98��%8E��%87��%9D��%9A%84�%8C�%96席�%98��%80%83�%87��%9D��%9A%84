            window.addEvent("load", function() {
            	var url = window.location.hash;
            	c_url = url.split('#');
            	id = 0;
            	tmp = 0;
            	if(c_url.length >= 1){		
            		for(i=1; i< c_url.length; i++){			
            			if(c_url[i].indexOf("jacommentid:") >-1){				
            				tmp = c_url[i].split(':')[1];				
            				if(tmp != ""){									
            					id = parseInt(tmp, 10);
            				}
            			}
            		}
            	}
            	            	
            	url = "http://www.chinacno.org/index.php?tmpl=component&option=com_jacomment&view=comments&contentoption=com_content&contentid=129&ran=" + Math.random();	            	            	        
            	if(id != 0){
					url += "&currentCommentID=" + id;
            	}
				var req = new Request({
					 method: 'get',
