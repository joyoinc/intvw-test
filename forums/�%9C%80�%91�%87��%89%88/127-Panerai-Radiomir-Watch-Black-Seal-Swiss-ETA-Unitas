		$(&quot;.hideul&quot;).eq(i).show().siblings(&quot;.hideul&quot;).hide();<br />
		$(&quot;.menu-mitop&quot;).stop();<br />
	});<br />
	$(&quot;.menu-mitop&quot;).mouseout(function(){<br />
		var i=$(&quot;.menu-mitop&quot;).index(this);<br />
		$(&quot;.hideul&quot;).eq(i).hide();<br />
	});<br />
	$(&quot;.hideul&quot;).mouseover(function(){<br />
		$(this).show();<br />
	});<br />
